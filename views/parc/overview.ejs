 <div class="container-fluid">
    <div class="row">
      <div class="col-xs-12">
      <div class="intro">
          <p>Hello Viktor, discover stars you want to follow.</p>
      </div>
      <div class="input-group">
        <input type="text" class="form-control searchicon" placeholder="Auf Fysam suchen...">
      </div><!-- /input-group -->
      <ul class="tabselection">
      <li>Top Stars</li>
      </ul>
      <div splashscreen></div>
      <div growl></div>
      </div>
      </div>
    </div>

  <div class="row">
    <div class="col-sm-6 col-md-4">
      <a class="thumbnail" href="/show/ria">
        <img src="http://fysam.de/images/1.jpg">
        <div class="caption">
          <h3>Rihanna</h3>
        </div>
      </a>
    </div>
  </div>
<div ng-repeat="star in stars track by $index">
	<div class="row">
	  <div class="col-sm-6 col-md-4">
	    <a class="thumbnail" href="/star/{{star.id}}">
	      <img src="http://img.celepedia.de/celepedia/rest/pictures/download/{{star.imgid}}?profile=SQUARE_300_2">
	      <div class="caption">
	        <h3>{{star.nickname}}</h3>
	      </div>
	    </a>
	  </div>
	</div>
</div>